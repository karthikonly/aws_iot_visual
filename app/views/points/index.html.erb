<div class="table-responsive">
  <h1>List of Points</h1>
  <table class="table-striped table-condensed table-bordered">
    <tr>
      <th>Time</th>
      <th>Value</th>
    </tr>
    <% @points.each do |point| %>
      <tr>
        <td><%= point.time %></td>
        <td><%= point.value %></td>
      </tr>
    <% end %>
  </table>
</div>